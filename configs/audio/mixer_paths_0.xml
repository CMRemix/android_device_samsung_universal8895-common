		<ctl name="ASRC1IN1R Input 1" value="LHPF2" />
		<ctl name="AIF1TX1 Input 1" value="ASRC1IN1L" />
		<ctl name="AIF1TX2 Input 1" value="ASRC1IN1R" />
		<ctl name="DMIC1 Switch" value="1" />
		<ctl name="DMIC2 Switch" value="1" />
