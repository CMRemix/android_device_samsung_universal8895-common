	<ctl name="ABOX Sampling Rate Mixer" value="48000" />
	<ctl name="ABOX Sampling Rate Out1" value="48000" />
	<ctl name="ABOX Sampling Rate Out2" value="48000" />

	<ctl name="ABOX SPUS ASRC0" value="On" />
